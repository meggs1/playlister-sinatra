<% if flash.has?(:notice) %>
  <%= flash[:notice] %>
<% end %>

<h1>Song: <%= @song.name %></h1>
Artist: <a href="/artists/<%= @song.artist.slug %>"><%= @song.artist.name %></a><br>
Genres:<% @song.genres.each do |genre| %>
  <a href="/genres/<%= genre.slug %>"><%= genre.name %></a><br> <br>
<% end %>
